<%= ui_filters @q, url: url_for do |f| %>
  <div class="form-group">
    <%= f.select :name_eq, @q_base.filter_map(&:name).uniq, { include_blank: true }, class: 'form-control selectpicker show-tick', title: t('activerecord.attributes.cyberwatch_config.name'), data: { 'live-search': "true" } %>
  </div>
  <div class="form-group">
    <%= f.select :url_eq, @q_base.filter_map(&:url).uniq, { include_blank: true }, class: 'form-control selectpicker show-tick', title: t('activerecord.attributes.cyberwatch_config.url'), data: { 'live-search': "true" } %>
  </div>
  <div class="form-group">
    <% opt = @q_base.flat_map(&:teams).uniq %>
    <%= render 'partials/multiple_select', target: 'teams_id', title: t('activerecord.attributes.cyberwatch_config.teams'), id: "cyberwatch_configs_filters_teams" do %>
      <% opt.each do |team| %>
      <option value='<%= team.id %>'><%= team.name %></option>
      <% end %>
    <% end %>
  </div>
<% end %>
