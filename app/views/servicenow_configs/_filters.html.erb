<%= ui_filters @q, url: url_for do |f| %>
  <div class="form-group">
    <%= f.select :name_eq, @q_base.filter_map(&:name).uniq, { include_blank: true }, class: 'form-control selectpicker show-tick', title: t('activerecord.attributes.servicenow_config.name'), data: { 'live-search': "true" } %>
  </div>
  <div class="form-group">
    <%= f.select :url_eq, @q_base.filter_map(&:url).uniq, { include_blank: true }, class: 'form-control selectpicker show-tick', title: t('activerecord.attributes.servicenow_config.url'), data: { 'live-search': "true" } %>
  </div>
  <div class="form-group">
    <% opts = ServicenowConfigExt.statuses_select %>
    <%= render 'partials/multiple_select', target: 'ext_status', title: t('activerecord.attributes.servicenow_config.status'), id: "servicenow_configs_filters_status" do %>
      <% opts.each do |label, key| %>
      <option value='<%= key %>'><%= label %></option>
      <% end %>
    <% end %>
  </div>
<% end %>
