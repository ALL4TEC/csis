<%= ui_filters @q, url: url_for do |f| %>
  <div class="form-group">
    <%= f.select :name_eq, @q_base.filter_map(&:name).uniq, { include_blank: true }, class: 'form-control selectpicker show-tick', title: t('activerecord.attributes.insight_app_sec_config.name'), data: { 'live-search': "true" } %>
  </div>
  <div class="form-group">
    <%= f.select :url_eq, @q_base.filter_map(&:url).uniq, { include_blank: true }, class: 'form-control selectpicker show-tick', title: t('activerecord.attributes.insight_app_sec_config.url'), data: { 'live-search': "true" } %>
  </div>
  <div class="form-group">
    <%= render 'partials/multiple_select', target: 'teams_id', title: t('activerecord.attributes.insight_app_sec_config.teams'), id: "insight_app_sec_config_filters_teams", opts: @q_base.flat_map(&:teams).uniq %>
  </div>
<% end %>
