<%= ui_filters @q, url: url_for do |f| %>
  <div class="form-group">
    <% opt = @q_base.filter_map(&:launcher).uniq %>
    <%= render 'partials/multiple_select', target: 'scan_configuration_launcher_id', title: t('activerecord.attributes.scan_launch.launcher'), id: "scan_launches_filters_launcher" do %>
      <% opt.each do |launcher| %>
      <option value='<%= launcher.id %>'>
        <%= launcher.full_name %>
      </option>
      <% end %>
    <% end %>
  </div>
  <div class="form-group">
    <% opt = @q_base.map(&:scan_type).uniq %>
    <%= render 'partials/multiple_select', target: 'scan_configuration_scan_type', title: t('activerecord.attributes.scan_launch.scan_type'), id: "scan_launches_filters_scan_type" do %>
      <% opt.each do |scan_type| %>
      <option value='<%= scan_type %>'>
        <%= scan_type %>
      </option>
      <% end %>
    <% end %>
  </div>
  <div class="form-group">
    <% opt = @q_base.map(&:target).uniq %>
    <%= render 'partials/multiple_select', target: 'scan_configuration_target', title: t('activerecord.attributes.scan_launch.target'), id: "scan_launches_filters_target" do %>
      <% opt.each do |target| %>
      <option value='<%= target %>'>
        <%= target %>
      </option>
      <% end %>
    <% end %>
  </div>
  <div class="form-group">
    <% opt = @q_base.map(&:kube_scan_id).uniq %>
    <%= render 'partials/multiple_select', target: 'kube_scan_id', title: t('activerecord.attributes.scan_launch.kubernetes_scan_id'), id: "scan_launches_filters_target" do %>
      <% opt.each do |id| %>
      <option value='<%= id %>'>
        <%= id %>
      </option>
      <% end %>
    <% end %>
  </div>
  <div class="form-group">
    <% opt = ScanLaunch.statuses %>
    <%= render 'partials/multiple_select', target: 'status', title: t('activerecord.attributes.scan_launch.status'), id: "scan_launches_filters_status" do %>
      <% opt.each do |label, key| %>
      <option value='<%= key %>'><%= ScanConfiguration.human_attribute_name("status.#{label}") %></option>
      <% end %>
    <% end %>
  </div>
<% end %>