<!-- User state -->
<%- state_clazz = user.discarded? ? 'text-danger' : state_color(user.state) %>
<i aria-hidden='true' class="material-icons align-middle <%= state_clazz %>" data-bs-toggle="<%= user.discarded? ? '' : 'tooltip' %>" data-bs-placement="right" title='<%= ApplicationRecord.human_attribute_name(user.state.to_s) %>'><%= icon %></i>
