<%- occ = builder.object %>
<%- uid = occ.id %>
<%- agg_kind = occ.kind_accro %>
<%- kind = KindUtil.from_accro(agg_kind) %>
<div class="d-flex flex-row align-items-center aggregate_occurrence_bloc_<%= agg_kind %> w-100 mt-1"
  data-target='filter.element' data-severity='<%= occ.vulnerability.severity %>' data-status='<%= occ.vulnerability.kind %>'
  data-kind='<%= kind %>' draggable='true' id="occ-bloc-<%= occ.id %>">
  <div class='d-flex flex-column justify-content-center handle-drag'>
    <i aria-hidden='true' class='material-icons align-middle'>open_with</i>
  </div>
  <div class="d-flex btn-group-vertical flex-column h-100">
    <%= render 'occurrences/filter_btn', kind: kind, action: 'title', agg_kind: agg_kind, uid: uid, data_value: occ.to_s %>
    <%= render 'occurrences/filter_btn', kind: kind, action: 'description', agg_kind: agg_kind, uid: uid, data_value: occ.vulnerability.diagnosis %>
    <%= render 'occurrences/filter_btn', kind: kind, action: 'solution', agg_kind: agg_kind, uid: uid, icone: 'add', data_value: occ.vulnerability.solution %>
  </div>
  <div class='form-check w-100 p-0'>
    <%= builder.check_box(class: 'form-check-input') %>
    <%= builder.label(class: 'form-check-label d-flex') do %>
      <%= render 'occurrences/occurrence', occ: occ %>
    <% end %>
  </div>
</div>
